{
  "jwt"         : {
    "key"       : "[SECRET]",
    "public"    : "[PUBLIC]",
    "expiresIn" : 1800 // 30 * 60 sec
  },
  "account"     : {
    "port"          : 8050,
    "ip-whitelist"  : [],
    "impersonate" : {
      "username" : "[GITHUB_USER_NAME]"
    },
    "oauth" : {
      "github"    : {
        "id"      : "[GITHUB_APP_IP]",
        "secret"  : "[GITHUB_APP_SECRET]"
      }
    }
  },
  "mongo"       : {
      "url"     : "mongodb://[USER]:[PASSWORD]@localhost/account"
  },
  "backend"     : {
      "mongodb"   : {
        "host"      : "localhost",
        "port"      : 27017,
        "user"      : "[USER]",
        "password"  : "[PASSWORD]"
      }
  }
}